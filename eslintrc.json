{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "react/prop-types": "warn",
    "semi": [2, "always"]
  }
}